{
    "name": "leafs/paystack",
    "description": "PayStack integration for Leaf Billing",
    "keywords": [
        "billing",
        "paystack",
        "payments",
        "leaf",
        "php"
    ],
    "homepage": "https://github.com/leafsphp/paystack",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Darko",
            "email": "mickdd22@gmail.com",
            "homepage": "https://mychi.netlify.app",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Leaf\\Billing\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "leafs/billing": "dev-main"
    },
    "require-dev": {
        "leafs/alchemy": "^2.2"
    },
    "scripts": {
        "alchemy": "./vendor/bin/alchemy setup",
        "test": "./vendor/bin/alchemy setup --test",
        "lint": "./vendor/bin/alchemy setup --lint",
        "actions": "./vendor/bin/alchemy setup --actions"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}